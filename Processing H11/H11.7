int[] GetallenLijst = {1, 1, 9, 2, 4, 6, 8, 1, 7, 2, 9, 9, 9};
int AantalGekozenGetal;

void setup(){
    TellHoeVaakGetal(1);
    TellHoeVaakGetal(9);
    
}


void TellHoeVaakGetal(int Gekozengetal){
    for(int i = 0; i < GetallenLijst.length; i++){
      if(GetallenLijst[i] == Gekozengetal){
       AantalGekozenGetal++;
      }
    }
    println(AantalGekozenGetal + "x " + Gekozengetal);
    
    
}